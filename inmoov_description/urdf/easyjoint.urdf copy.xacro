<?xml version="1.0"?>
<robot name="inmoov"> 
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/> <!-- This ia the r2d2 cylinder.  The body of most robots -->
      </geometry>
    </visual>
  </link>

  <!-- Now we define a joint to link the two together -->
  <joint name="base_to_right_leg" type="continuous">
    <parent link="base_link"/>
    <child link="right_leg"/>
    <axis xyz="0.0 1.0 0.0"/>
    <origin xyz="0.0 -0.25 0.25"/>
    <limit lower="-1.0" upper="1.0" effort="1000.0" velocity="0.5"/>
  </joint>


  <link name="right_leg">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>

    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>  <!-- this is a box that is 0.6m x 0.1m x 0.2m -->
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.8"/>
    </visual>
  </link>

  <!-- Now we define a joint to link the two together -->
  <joint name="right_leg_to_right_foot" type="continuous">
    <parent link="right_leg"/>
    <child link="right_foot"/>
    <origin xyz="0.0 -0.25 0.25"/>
  </joint>
  


  <link name="right_foot">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>
    
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>  <!-- this is a box that is 0.6m x 0.1m x 0.2m -->
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
    </visual>
  </link>




</robot>